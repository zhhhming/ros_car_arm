<launch>
    <include file="$(find car_arm)/move_launch/launch/amcl_diff.launch" />
    <include file="$(find car_arm)/move_launch/launch/mapserver.launch" />

    <!-- 运行move_base节点 -->
    <include file="$(find car_arm)/move_launch/launch/setting.launch" />
    <!-- 运行rviz -->
    <node pkg="tf2_ros" type="static_transform_publisher" name="static_transform_publisher" args="0 0 0 -1.57 0 -1.57 /camera /camera_frame" />
    <node pkg="joint_state_publisher" name="joint_state_publisher" type="joint_state_publisher" />
    <!-- <node pkg="robot_state_publisher" name="robot_state_publisher" type="robot_state_publisher" /> -->
    <!-- <node pkg="rviz" type="rviz" name="rviz" args="-d $(find my_car_navigation)/config/move.rviz" /> -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find my_car_navigation)/config/plan_show.rviz" />

</launch>
