<launch>

    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
        <rosparam file="$(find car_arm)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find car_arm)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find car_arm)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find car_arm)/param/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find car_arm)/param/base_local_planner_params.yaml" command="load" />
        <rosparam file="$(find car_arm)/param/move_base_params.yaml" command="load" />
         <remap from="cmd_vel" to="/mobile_base_controller/cmd_vel"/>
    </node>

</launch>
