<launch>
  <!-- Run the teleop node -->
  <node pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" name="teleop_twist_keyboard" output="screen">
    <!-- Set the speed parameter -->
    <param name="_speed" value="0.3" />
    <!-- Set the turn parameter -->
    <param name="_turn" value="0.5" />
  </node>
</launch>
